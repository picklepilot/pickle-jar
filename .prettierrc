{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "plugins": ["prettier-plugin-tailwindcss"],
    "tailwindAttributes": ["theme"],
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed"
}
